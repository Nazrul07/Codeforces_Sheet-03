#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int test,i;
    cin>>test;
    int a[test];
    for(i=0 ; i<test ; i++)
    {
        cin>>a[i];
        if(a[i]>0)
         cout<<"1"<<" ";
        else if(a[i]<0)
         cout<<"2"<<" ";
        else
          cout<<"0"<<" ";
    }
}
