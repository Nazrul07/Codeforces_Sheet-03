#include<stdio.h>
int main()
{
    int n,sum=0,sum1=0,i;
    scanf("%d",&n);
    int a[n],arr[n];
    for(i=0 ; i<n ; i++)
    {
        scanf("%d",&a[i]);
        sum=sum+a[i];
    }
    for(i=0 ; i<n ; i++)
    {
        scanf("%d",&arr[i]);
        sum1=sum1+arr[i];
    }
    if(sum==sum1)
        printf("yes");
    else
        printf("no");
    return 0;
}
